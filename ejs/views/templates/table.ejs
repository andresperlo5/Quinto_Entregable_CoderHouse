<table class="table table-dark table-striped container">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Titulo</th>
            <th scope="col">Precio</th>
            <th scope="col">Thumbnail</th>
        </tr>
    </thead>
    <tbody>
        <% data.forEach(function(i){ %>
            <tr>
                <th scope="row">
                    <%= i.id%>
                </th>
                <td>
                    <%= i.title%>
                </td>
                <td>
                    <%= i.price%>
                </td>
                <td>
                    <div class="card"   >
                        <img src="<%= i.thumbnail%>" class="card-img-top" alt="...">
                    </div>
                    </div>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>